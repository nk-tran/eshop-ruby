
<h1>Signup!</h1>

<%= form_for :user, url: '/users' do |f| %>

  Name: <br><%= f.text_field :name %><br>
  Email: <br><%= f.text_field :email %><br>
  Password: <br><%= f.password_field :password %><br>
  Confirm Password: <br><%= f.password_field :password_confirmation %><br>
  <%= f.submit "Submit" %>

<% end %>
